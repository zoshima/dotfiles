set number relativenumber 
set expandtab 
set tabstop=2 
set shiftwidth=0
set splitbelow 
set splitright 
set pumheight=5  
set completeopt=menuone,noselect,fuzzy 
set clipboard=unnamedplus
set hlsearch
set path=,,**/*
set incsearch
set signcolumn=number
set cursorline
set autoindent
set wildmenu wildoptions=pum
set noswapfile 
set viminfofile=NONE

syntax on

" ~/.vim/colors/custom.vim
colorscheme custom
